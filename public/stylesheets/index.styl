@charset "UTF-8";
/* Main CSS Document */
html, body, #content {
   	height: 100%;
   	margin: 0px;
   	padding: 0px
}

#leaderboard {
  	background-color: #28B3B1;
    height: 100%;
  	float: left;
    width: 15%;
    border-right: solid;
    border-width: medium;
}

#content {
    float: left;
    width: 84%;
    height: 100%;
}

#map-canvas {
  	float: left;
  	width: 100%;
    height: 75%;
    background-color: #880000;

}
#geoDiv {
    background-color: #00B7FF;
    height:25%;
    width: 50%;
    float: left;
}

#mag-canvas {
    float: left;
    width: 50%;
    height: 25%;
    margin: 0;
    padding: 0;
    background-color: #330033;
}
